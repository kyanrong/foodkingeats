    "bootstrap": "^4.5.0",
    "connected-react-router": "^6.8.0",
    "elasticsearch": "^16.7.1",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "lodash.groupby": "^4.6.0",
    "normalizr": "^3.6.0",
    "pg": "^8.1.0",
    "prop-types": "^15.7.2",
    "react": ">=15",
    "react-dom": "^16.13.1",
    "react-dom-factories": "^1.0.2",
    "react-hook-form": "^5.7.2",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
