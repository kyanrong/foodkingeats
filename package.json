  "license": "MIT",
  "scripts": {
    "start": "rm -rf dist/ && yarn build:client && cp src/server.js dist/ && node dist/server.js",
    "start:local": "node src/server.js",
    "build:client": "parcel build src/index.html --no-source-maps --out-dir dist/",
    "server": "nodemon --exec 'heroku local' --signal SIGTERM",
    "work": "concurrently 'yarn server' 'parcel src/index.html'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material/react-text-field": "^0.15.0",
    "elasticsearch": "^16.7.1",
    "express": "^4.17.1",
    "lodash.groupby": "^4.6.0",
    "normalizr": "^3.6.0",
    "parcel-bundler": "^1.12.4",
    "pg": "^8.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dom-factories": "^1.0.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.0"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.3"
  }
}
